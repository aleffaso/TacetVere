<main id="pageform">
    <section class="login">
        <div class="container d-flex justify-content-center">
            <div class="card text-dark bg-light text-center">
                <div class="card-header">
                    <img src="assets/img/logo_blue.png">
                </div>
                <div class="card-body">
                    <form method="POST" action="/login">
                        <input class="form-control" name="email" type="email" placeholder="email@exemplo.com" required>
                        <br>
                        <input class="form-control" name="password" type="password" placeholder="Senha" required>
                        <br>
                        <button type="submit" class="btn btn-send">Login</button>
                    </form>
                </div>
            </div>
        </div>
    </section>
</main>

